md-dialog(aria-label="Thank you")
		md-toolbar
			.md-toolbar-tools
				h2 Thank you
				span(flex)
				md-button.md-icon-button(ng-click="okay()")
					md-icon.icon-white(md-svg-src="public/shared/img/icons/close.svg" aria-label="Close dialog")

		md-dialog-content
			p.padding-standard Thank you for your feedback! This information will be used to determine our development priorities and the future of this community. 
		
		md-dialog-actions(layout="row")
			span(flex)
			md-button.md-primary.md-raised(ng-click="okay()") Okay