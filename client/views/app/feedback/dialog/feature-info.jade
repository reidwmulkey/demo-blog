md-dialog(aria-label="User agreement")
	form(name="agreementForm")
		md-toolbar
			.md-toolbar-tools
				h2 Feature Information
				span(flex)
				md-button.md-icon-button(ng-click="cancel()")
					md-icon.icon-white(md-svg-src="public/shared/img/icons/close.svg" aria-label="Close dialog")

		md-dialog-content
			h3 {{description}}
			p {{info}}
		
		md-dialog-actions(layout="row")
			span(flex)
			md-button.md-primary.md-raised(ng-click="okay()") Okay